
<div class="row">
  <div class="col s6">
    <ul class="collection team-list">
      {{#each model as |team|}}
        {{#link-to 'teams.team' team.id tagName="li" class="collection-item team-item"}}
          {{team.teamName}}
        {{/link-to}}
      {{/each}}
    </ul>
  </div>
  <div class="col s6">
    <div class="card blue-grey darken-1">
      {{outlet}}
    </div>
  </div>
</div>
